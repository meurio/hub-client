  },
  "bugs": {
    "url": "https://github.com/meurio/hub-client/issues"
  },
  "scripts": {
    "coverage": "nyc report -r=html",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "watch-client": "NODE_ENV=development UV_THREADPOOL_SIZE=100 NODE_PATH='./src' API_URL='http://localhost:3000' APP_DOMAIN='localhost.dev:3001' PAGARME_KEY='ek_test_PYsS1XrZsCCF7wynC67YEi5RW3lSCV' node webpack/webpack-dev-server.js",
    "start-dev": "NODE_ENV=development NODE_PATH=./src API_URL=http://localhost:3000 APP_DOMAIN=localhost.dev:3001 PAGARME_KEY=ek_test_PYsS1XrZsCCF7wynC67YEi5RW3lSCV NEW_RELIC_HOME=./src node ./bin/server.js",
    "start-profiler": "NODE_ENV=development NODE_PATH=./src API_URL=http://localhost:3000 APP_DOMAIN=localhost.dev:3001 PAGARME_KEY=ek_test_PYsS1XrZsCCF7wynC67YEi5RW3lSCV NEW_RELIC_HOME=./src node --inspect ./bin/server.js",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "profiler": "concurrently --kill-others \"npm run watch-client\" \"npm run start-profiler\"",
    "test": "./node_modules/.bin/karma start --single-run && npm run test:unique",
    "test:unique": "./node_modules/.bin/mocha --require ./app/scripts/tests/dom.js $(find ./app -regex '.*.spec.js') --compilers js:babel-core/register",
    "test:reporter": "./node_modules/.bin/mocha --require ./app/scripts/tests/dom.js $(find ./app -regex '.*.spec.js') --compilers js:babel-core/register --reporter mocha-junit-reporter",
    "test:w": "npm run test:unique -- --watch",
    "test:single": "./node_modules/.bin/mocha --require ./app/scripts/tests/dom.js $(find ./app/*/$DIR -name $CLASSNAME.spec.js) --compilers js:babel-core/register",
    "test:single:w": "npm run test:single -- -w",
    "postinstall": "npm run build"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "API_URL": "http://localhost:3000",
        "APP_DOMAIN": "localhost.dev:3001",
        "PAGARME_KEY": "ek_test_PYsS1XrZsCCF7wynC67YEi5RW3lSCV",
        "NEW_RELIC_HOME": "./src"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "API_URL": "http://localhost:3000",
        "APP_DOMAIN": "localhost.dev:3001",
        "PAGARME_KEY": "ek_test_PYsS1XrZsCCF7wynC67YEi5RW3lSCV"
      }
    }
  },
