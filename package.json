{
  "name": "bonde-client",
  "version": "0.3.1",
  "description": "[Nossas][Bonde] Node.js Server and React + Redux Progressive Web App",
  "author": "nossas",
  "homepage": "http://bonde.org",
  "scripts": {
    "test": "BABEL_DISABLE_CACHE=1 node --max_old_space_size=2048 ./node_modules/.bin/jest --config ./tools/jest.config.json  --maxWorkers=2",
    "test:a": "BABEL_DISABLE_CACHE=1 ./node_modules/.bin/jest --config ./tools/jest.config.json",
    "test:w": "yarn test:a -- --watch",
    "coverage": "nyc npm test",
    "start": "cross-env NODE_ENV=development nodemon -r 'babel-register' --delay 1 --watch ./server --watch ./tools ./server",
    "start:prod": "cross-env NODE_ENV=production node ./build/server.js",
    "heroku-postbuild": "./node_modules/.bin/webpack --progress --config ./tools/webpack.client.js && ./node_modules/.bin/webpack --progress --config ./tools/webpack.server.js",
    "clean": "rm -rf build",
    "routes": "grep -rhe path: ./routes/$inner | sed -e \"s/path: '\\([a-zA-Z\\/:_]*\\)',/\\1/g\" | sort | uniq"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanyesmunt/universal-js.git"
  },
