{
  "name": "bonde-client",
  "version": "0.5.1",
  "description": "Bonde Client - Node.js Server and React + Redux Progressive Web App",
  "author": "Nossas <tech@nosssas.org>",
  "homepage": "http://bonde.org",
  "scripts": {
    "test": "node --max_old_space_size=3584 ./node_modules/.bin/jest --config ./tools/jest/config.json --maxWorkers=2",
    "test:w": "yarn test -- --watch",
    "coverage": "nyc npm test",
    "start": "nodemon -r 'babel-register' --delay 1 --watch ./server --watch ./tools ./server",
    "start:prod": "node ./build/server.js",
    "heroku-postbuild": "./node_modules/.bin/webpack --verbose --config ./tools/webpack.client.js && ./node_modules/.bin/webpack --verbose -p --config ./tools/webpack.server.js",
    "clean": "rm -rf build",
    "routes": "grep -rhe path: ./routes/$inner | sed -e \"s/path: '\\([a-zA-Z\\/:_]*\\)',/\\1/g\" | sort | uniq",
    "release": "./node_modules/.bin/standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nossas/bonde-client.git"
  },
  "keywords": [],
